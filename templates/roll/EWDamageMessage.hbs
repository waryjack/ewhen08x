<form class="ewhen roll-dialog">
 <header class="roll-header taskroll-info">
        <h1 class="roll-name">{{localize "EW.rolltype.damageroll"}}
            <div class="taskroll-final {{outclass}}" style="">
                {{roll.total}}
            </div>
        </h1>
</header>
<div class="dmgroll-msg taskroll-info">
    <p> <strong>
        <img style="float:left; margin-right:12px; vertical-align:center;" src="{{d.wpnImg}}" height="48" width="48">
        {{d.wpnName}}<br>Damage: {{localize d.wpnDmg}}{{d.friendlyExt}} {{#if (ne d.mods 0)}}+{{d.mods}}{{/if}}{{#if (lt d.mods 0)}}{{d.mods}}{{/if}}
        </strong>
    </p>
</div>
<div class="taskroll-tt">
    <hr>
    <div>{{tooltip}}</div>
</div>