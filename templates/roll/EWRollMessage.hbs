{{log "Roll received by template: " roll}}
{{log "Roll.total: " roll.total}}
{{log "this.result from calling class: " rollTotal}}
<form class="ewhen roll-dialog">
 <header class="roll-header taskroll-info">
        <h1 class="roll-name">{{#proper (setting (getCustomName d.chosenAttribute))}}{{/proper}}{{!--localize (proper d.chosenAttribute)--}} Roll
             {{#if (eq outcome "Mighty Success!")}}<a data-actor-id="{{actor}}" id="legendize" class="taskroll-final {{outclass}}" title="Make Legendary!"><i class="fas fa-fist-raised"></i></a>{{/if}}
            <div class="taskroll-final {{outclass}}" style="">
                {{rollTotal}}
            </div>
        </h1>
</header>
<div class="taskroll-msg">
    <p> <strong>{{#proper (setting (getCustomName d.chosenAttribute))}}{{/proper}} ({{d.attrVal}})</strong>
        {{#if (ne d.chosenCombat "none")}} <strong>+ {{#proper (setting (getCustomName d.chosenCombat))}}{{/proper}} ({{d.comVal}})</strong>{{/if}}
        {{#if (ne d.chosenCareer "none")}} <strong>+ {{d.chosenCareer}} ({{d.cVal}})</strong>{{/if}}
        {{#if (ne d.mods 0)}} {{#if (gt d.mods 0)}}<strong>+</strong>{{/if}} <strong>{{d.mods}}</strong>{{/if}}
    </p>
    <p class="taskroll-dmods">
        <strong>{{localize "EW.game_term.difficulty"}}: {{localize d.diffStr}}</strong>  <!--span><strong>{{localize "EW.game_term.dice"}}:</strong><i class="fas fa-dice-d6"></i><i class="fas fa-dice-d6"></i>{{#times d.bdNum}}<i class="fas fa-dice-d6" style="color:rgb(10, 80, 10);"></i>{{/times}}{{#times d.pdNum}}<i class="fas fa-dice-d6" style="color:rgb(160, 2, 2);"></i>{{/times}}</span-->
    </p>
</div>
<div class="taskroll-tt">
    <hr>
    <div>{{tooltip}}</div>
</div>
<div class="taskroll-outcome {{outclass}}">
    {{outcome}}
</div>

</form>